<UserControl x:Class="Projekt_WPF_Solution.Control.AutoCompleteTextBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Projekt_WPF_Solution.Control"
             mc:Ignorable="d" MinWidth="50" MinHeight="25">
    <Grid>
        <ComboBox Name="ComboBox" IsSynchronizedWithCurrentItem="True" IsTabStop="False" SelectionChanged="AutoCompleteComboBox_SelectionChanged"/>
        <TextBox Name="TextBox" VerticalContentAlignment="Center" TextChanged="TextBox_TextChanged"/>


    </Grid>
</UserControl>
